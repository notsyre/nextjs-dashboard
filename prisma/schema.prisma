generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cesantia {
  id               Int      @id @default(autoincrement())
  date             DateTime @default(now())
  email            String
  name             String
  lastName         String
  cedula           Int
  expedicion       String
  destinacion      String
  direccion        String
  cesantias        String
  valor            Int
  empresacesantias Int?
  documentos       String
  firma            String?    
}
